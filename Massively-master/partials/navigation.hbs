<nav id="nav">
    <ul class="links">
        {{#foreach navigation}}
        <li class="nav-{{slug}} {{#if current}}active{{/if}}"><a href="{{url absolute="true"}}">{{label}}</a></li>
        {{/foreach}}
    </ul>
    <ul class="icons">
        {{#if @site.twitter}}
        <li><a href="{{twitter_url @site.twitter}}" class="icon fa-twitter" title="Twitter"><span
                    class="label">Twitter</span></a></li>
        {{/if}}
        <li><a href="{{twitter_url @site.twitter}}" class="icon fa-instagram" title="Instagram"><span
                    class="label">Instagram</span></a></li>
        {{#if @labs.members}}
        <li><a class="subscribe-button" href="#subscribe">Subscribe</a></li>
        {{/if}}
    </ul>

</nav>